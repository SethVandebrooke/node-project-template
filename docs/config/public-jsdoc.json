{
    "plugins": [
        "plugins/markdown"
    ],
    "recurseDepth": 10,
    "source": {
        "include": ["./src/","./docs/config/public-homepage.md"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "systemName": "Calculator",
        "cleverLinks": false,
        "monospaceLinks": false,
        "custom": {
            "siteName": "Calculator"
        }
    },
    "opts": {
        "template": "./docs/templates/public-docs-template",
        "encoding": "utf8",
        "destination": "./docs/public-docs/",
        "recurse": true,
        "tutorials": "./docs/tutorials/"
    }
}