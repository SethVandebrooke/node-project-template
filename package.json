{
  "name": "calculator",
  "version": "1.0.0",
  "description": "Jump start to creating a nodejs project with testing, documentation, and reports.",
  "main": "index.js",
  "scripts": {
    "dependency-audit": "npm dedupe && npm prune && npm audit fix",
    "js-code-metric": "./node_modules/.bin/js-code-metric -r -d ./docs/metrics src",
    "jsdoc": "jsdoc -c ./jsdoc.json",
    "generate-docs": "npm run test && npm run js-code-metric && npm run jsdoc ",
    "test": "jest --coverage --coverageDirectory=\"./docs/\"",
    "build": "babel src -d temp && grunt",
    "start": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SethVandebrooke/node-project-template.git"
  },
  "keywords": [],
  "author": "Seth Vandebrooke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SethVandebrooke/node-project-template/issues"
  },
  "homepage": "https://github.com/SethVandebrooke/node-project-template#readme",
  "devDependencies": {
    "jest": "^25.5.3",
    "js-code-metric": "^1.0.76",
    "jsdoc": "^3.6.4",
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "grunt": "^1.0.3",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-uglify": "^4.0.0"
  },
  "dependencies": {
    "ink-docstrap": "^1.3.2"
  }
}
